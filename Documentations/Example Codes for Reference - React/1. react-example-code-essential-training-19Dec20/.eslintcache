onst dishObjects = dishes.map((dish,i) => ({id: i, title: dish}))\nconsole.log(dishObjects)\n\ndishes.map((dish) => console.log(dish));\n\nfunction AppOne() {\n  return (\n    <div className=\"App\">\n      <Header name=\"Horacio\"/>\n      <Main adjective=\"amazing\" dishes={dishObjects}/> \n      <Footer year={new Date().getFullYear()} />\n    </div>\n  );\n}\n\nfunction App( {login} ) {\n  const [data, setData] = useState(null);\n  const [loading, setLoading] = useState(false);\n  const[error, setError] = useState(null);\n\n  useEffect(() => {\n    if (!login) return; // jump out\n    setLoading(true);\n\n    fetch(`https://api.github.com/users/${login}`)\n      .then((response) => response.json())\n      .then(setData)\n      .then(() => setLoading(false))\n      .catch(setError);\n  }, [login]); // [login] so useEffect is called when the login value changes \n\n  if (loading) return <h1> Loading...</h1>;\n  if (error) return <pre>JSON.stringify(error,null,2) </pre>;\n  if (!data) return null;\n\n  if (data){\n    return (\n    <div>\n      <h1>{data.login}</h1>  \n      <img alt={data.login} src={data.avatar_url} />\n    </div>\n    );\n  }\n\n  return <div>No User Available</div>; \n}\n\n// function SecretComponent() {\n//   return <h1>Super secret informaiton for authorized users only.</h1>\n// }\n\n// function RegularComponent() {\n//   return <h1>Everyone can see this component.</h1>\n// }\n\n// function App() {\n//   const [checked, toggle] = useReducer((checked) => !checked, false);\n\n//   return (\n//     <>\n//       <input type=\"checkbox\" \n//       value={checked} \n//       onChange={toggle}\n//       />\n//       <p>{checked ? \"checked\" : \"not checked\"}</p>\n//     </>\n//   );\n// }\n\n  // const [emotion, setEmotion] = useState(\"happy\");\n  // const [secondary, setSecdonary] = useState(\"tired\");\n\n  // useEffect(() => {\n  //   console.log(`It's ${emotion} around here!`);\n  // }, [emotion]);\n\n  // useEffect(() => {\n  //   console.log(`It's ${secondary} around here!`);\n  // }, [secondary]);\n\n\n      {/* <h1>Current emotion is {emotion} and {secondary}.</h1>\n      <button onClick={() => setEmotion(\"happy\")}>\n        Happy\n      </button>\n      <button onClick={() => setSecdonary(\"crabby\")}>\n        Make Crabby\n      </button>\n      <button onClick={() => setEmotion(\"frustrated\")}>\n        frustrate\n      </button>\n      <button onClick={() => setEmotion(\"enthusastic\")}>\n        enthuse\n      </button> */}\n\n// function App(props) {\n//   return (\n//     <>\n//     {props.authorized ? <SecretComponent /> : <RegularComponent /> }\n//     </>\n//   )\n// }\n\nexport default App;\n",{"ruleId":"17","replacedBy":"18"},{"ruleId":"19","replacedBy":"20"},{"ruleId":"21","severity":1,"message":"22","line":1,"column":37,"nodeType":"23","messageId":"24","endLine":1,"endColumn":47},{"ruleId":"21","severity":1,"message":"25","line":49,"column":10,"nodeType":"23","messageId":"24","endLine":49,"endColumn":16},{"ruleId":"26","severity":1,"message":"27","line":125,"column":7,"nodeType":"28","messageId":"29","endLine":137,"endColumn":20},"no-native-reassign",["30"],"no-negated-in-lhs",["31"],"no-unused-vars","'useReducer' is defined but never used.","Identifier","unusedVar","'AppOne' is defined but never used.","no-lone-blocks","Block is redundant.","BlockStatement","redundantBlock","no-global-assign","no-unsafe-negation"]